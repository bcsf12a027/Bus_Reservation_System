        <style type="text/css">
            .modalDialog {
		position: fixed;
		font-family: Arial, Helvetica, sans-serif;
		top: 0;
		right: 0;
		bottom: 0;
		left: 0;
		background: rgba(0,0,0,0.8);
		z-index: 99999;
		opacity:0;
		transition: opacity 400ms ease-in;
		pointer-events: none;
	}

	.modalDialog:target {
		opacity:1;
		pointer-events:all;
	}

	.modalDialog > div {
		width: 400px;
		position: relative;
		margin: 5% auto;
		padding: 5px 20px 13px 20px;
		border-radius: 10px;
		background: -webkit-linear-gradient(#fff, #999);
	}

	.close {
		position: absolute;
		right: 0px;
		text-align: center;
		top: 0px;
		width: 24px;
		font-weight: bold;
		border-radius: 12px;
		box-shadow: 1px 1px 3px #000;
        color:red;
	}

	.close:hover { background-color:darkgrey; }

    </style>
    <script>
        $(document).ready(function () {
            $("#lgn").click(function () {
                document.getElementById("usrn").value = "";
                document.getElementById("pwd").value = "";
                document.getElementById("c1").checked = false;
            });
            $("#gtm").click(function () {
                document.getElementById("newpssd").value = "";
                document.getElementById("pssd").value = "";
            });
            $("#nm").click(function () {
                document.getElementById("newpssd").value = "";
                document.getElementById("pssd").value = "";
            });
            $("#reg").click(function () {
                if ($("#pssd").val() != $("#newpssd").val()) {
                    alert("Mis-Match Error");
                    document.getElementById("newpssd").value = "";
                    document.getElementById("pssd").value = "";
                }
            });
        });
    </script>
